{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Cortex Debug",
			"cwd": "${workspaceRoot}",
			"executable": "${workspaceRoot}/build/stm32_sleet.elf",
			"svdFile": "${workspaceRoot}/JLink/STM32L100.svd",
			"request": "launch",
			"type": "cortex-debug",
			"servertype": "jlink",
			"device": "Cortex-M3",
			"interface": "swd",
			"showDevDebugOutput": false,
			"ipAddress": null,
		            "serialNumber": null,
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 8000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    { "type": "console", "label": "ITM", "port": 0 }
                ]
            },

		}
	]
}